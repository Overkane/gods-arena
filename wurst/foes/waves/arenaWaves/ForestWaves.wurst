  ..setDamageBonus(1, 10)
  ..setDurationNormal(1, 0.25)
  ..setDurationHero(1, 0.25)

  new AbilityDefinitionWarStompcreep(OWLBEAR_CLAP)
  ..setDamage(1, 175)
  ..setManaCost(1, 100)
  ..setCooldown(1, 14)
  ..setDurationNormal(1, 2)
  ..setDurationHero(1, 2)


