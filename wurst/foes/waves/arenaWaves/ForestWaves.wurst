  ..setCooldown(1, 14)
  ..setDurationNormal(1, 6)
  ..setDurationHero(1, 6)

  new AbilityDefinitionBashcreep(SASQUATCH_BASH)
  ..setChancetoBash(1, 25)
  ..setDamageBonus(1, 10)
  ..setDurationNormal(1, 0.75)
  ..setDurationHero(1, 0.75)

  new AbilityDefinitionWarStompcreep(SASQUATCH_CLAP)
