package CreepObjEditing
import public UnitObjEditing

public class CreepDefinition extends UnitDefinition
  int exp = 10

  construct(int newId, int origId)
    super(newId, origId)
    setDefaults()

  function setDefaults()
    setAcquisitionRange(9000)
    setGoldBountyAwardedBase(0)
    setLevel(1)
    setMovementType(MovementType.Foot)
    setAttack1DamageSidesperDie(1)
    setAttack1DamageNumberofDice(1)
    setCollisionSize(31)